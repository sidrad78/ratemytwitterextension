{
    "name" : "Getting Started Example",
    "description" : "Build an Extension!",
    "version": "1.0",
    "manifest_version" : 3,
    "background" : {
        "service_worker" : "service.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon": {
            "16": "/images/celticsbird16.png",
            "32": "/images/celticsbird32.png",
            "48": "/images/celticsbird48.png",
            "128": "/images/celticsbird128.png"
          }
    },
    "icons" : {
        "16": "/images/celticsbird16.png",
        "32": "/images/celticsbird32.png",
        "48": "/images/celticsbird48.png",
        "128": "/images/celticsbird128.png"
    }
}